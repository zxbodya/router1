  },
  "homepage": "https://github.com/zxbodya/router1#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
